{
  "name": "v11-geckos-team-03",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.6.4",
    "axios": "^0.19.1",
    "http-proxy-middleware": "^0.20.0",
    "memoize-one": "^5.1.1",
    "postcss-cli": "^6.1.3",
    "react": "^16.9.0",
    "react-dnd": "^9.4.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "watch:sass": "node-sass src/sass/index.scss src/index.css -watch",
    "compile:sass": "node-sass src/sass/index.scss src/index.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/index.css -o src/index.css",
    "start:react": "react-scripts start",
    "start": "npm-run-all --parallel start:react watch:sass",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5"
  },
  "homepage": "http://signiorgratiano.github.io/v11-geckos-team-03"
}
